@model List<MVC_Voorbeeld3.Models.Filiaal>
@{
    ViewBag.Title = "Filialen";
}

<h2>Filialen</h2>
<ul>
    @foreach (var filiaal in Model)
    {
        <li>@filiaal.ID @filiaal.Naam  @Html.DisplayFor(m => filiaal.Gebouwd) @Html.DisplayFor(m => filiaal.Waarde) @Html.DisplayFor(m => filiaal.Eigenaar) </li>
    }
</ul>
